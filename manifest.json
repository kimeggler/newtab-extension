{
  "manifest_version": 2,
  "name": "New-Tab",
  "description": "Beautiful New-Tab-Extension",
  "version": "1",
  "author": "Kim Eggler",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Hello Kim"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://unpkg.com/axios/dist/axios.min.js; object-src 'self'",
  "permissions": [
    "activeTab"
  ]
}
